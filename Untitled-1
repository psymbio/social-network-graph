// editing

void delete_user(struct Graph *graph, int a, int b)
{
    if (find_user(graph, a) && find_user(graph, b))
    {
        struct user_node *aCrawl = graph->array[a].head;
        while (aCrawl)
        {
            if(aCrawl->dest == a)
                free(graph->array);
            aCrawl = aCrawl->next;
        }
        /*struct user_node *bCrawl = graph->array[b].head;
        while (bCrawl)
        {
            if(bCrawl->dest == b)
                free(graph->array);
            bCrawl = bCrawl->next;
        }*/
    }
}